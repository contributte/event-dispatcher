{
  "name": "contributte/event-dispatcher",
  "description": "Best event dispatcher / event manager / event emitter for Nette Framework",
  "keywords": [
    "nette",
    "symfony",
    "event",
    "dispatcher",
    "emitter"
  ],
  "type": "library",
  "license": "MIT",
  "homepage": "https://github.com/contributte/event-dispatcher",
  "authors": [
    {
      "name": "Milan Felix Å ulc",
      "homepage": "https://f3l1x.io"
    }
  ],
  "require": {
    "php": ">=7.2",
    "nette/di": "^3.0.0",
    "symfony/event-dispatcher": "^4.3.1 || ^5.0.0 || ^6.0.0"
  },
  "require-dev": {
    "contributte/qa": "^0.4",
    "contributte/tester": "^0.3",
    "contributte/phpstan": "^0.1",
    "mockery/mockery": "^1.5.0"
  },
  "conflict": {
    "nette/di": "<3.0.0"
  },
  "autoload": {
    "psr-4": {
      "Contributte\\EventDispatcher\\": "src"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Tests\\": "tests"
    }
  },
  "minimum-stability": "dev",
  "prefer-stable": true,
  "config": {
    "sort-packages": true,
    "allow-plugins": {
      "dealerdirect/phpcodesniffer-composer-installer": true
    }
  },
  "extra": {
    "branch-alias": {
      "dev-master": "0.9.x-dev"
    }
  }
}
